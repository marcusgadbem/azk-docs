{
  "title": "azk Docs Gitbook",
  "description": "Gitbook documentation for azk",
  "version": "2.6.7",
  "azkVersion": "0.13.0",
  "releaseNotes": "http://......",
  "plugins": [
    "ga",
    "hotjar",
    "include",
    "scrollablecode",
    "-sharing"
  ],
  "pluginsConfig": {
    "hotjar": {
      "hjid": "HOTJAR_ID"
    },
    "ga": {
      "token": "GA_UA_ID",
      "configuration": {
        "cookieDomain": "GA_COOKIE_DOMAIN"
      }
    },
    "sharing": {
      "facebook": true,
      "twitter": true,
      "google": false,
      "weibo": false,
      "instapaper": false,
      "vk": false,
      "all": [
        "facebook", "google", "twitter",
        "weibo", "instapaper"
      ]
    }
  },
  "links": {
    "home": "http://azk.io",
    "issues": "https://github.com/azukiapp/azk/issues",
    "theme": "node_modules/gitbook-theme-clarity/lib",
    "custom": {
        "custom": "http://azukiapp.com"
    }
  }
}
